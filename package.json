{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "A webpack boilerplate for starting webpack projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "lint": "eslint ./src --fix",
    "format": "prettier --write ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwpray/webpack-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "babel",
    "eslint",
    "prettier",
    "husky",
    "@commitlint",
    "jest"
  ],
  "author": "Nick Pray <nickthepray@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nwpray/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/nwpray/webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run format && npm test",
      "pre-push": "npm run lint && npm run format && npm test"
    }
  }
}
